#!/bin/sh
# Copyright (C) 2000-2016 Synology Inc. All rights reserved.

case $1 in
	start)
		### Start this package.
		pkgext "Start"
		exit 0
	;;
	stop)
		### Stop this package.
		pkgext "Stop"
		exit 0
	;;
	status)
		### Check package alive.
		if [ -h "/usr/bin/pkgext" ]; then
			exit 0
		else
			exit 1
		fi
	;;
	killall)
        ;;
	log)
		exit 0
	;;
esac

